<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="PhenoTips.TestReport" table="testreport">
    <id name="reportId" type="long" unsaved-value="undefined">
      <column name="report_id" not-null="true" />
      <generator class="assigned" />
    </id>
    <property name="patientId" type="string" column="patient_id" />
    <property name="filepath" type="string" column="filepath" />
    <property name="date" type="long" column="date" />
    <property name="columnCount" type="integer" column="column_count" />
    <list name="columnOrder" table="testreport_columnorder">
      <key column="qid"/>
      <list-index column="list_order"/>
      <element column="column_order" type="string"/>
    </list>
    <list name="data" table="testreport_data">
      <key column="qid"/>
      <list-index column="data_order"/>
      <element column="data" type="string"/>
    </list>
  </class>
</hibernate-mapping>
