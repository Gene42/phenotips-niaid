    <column_order>
      <cache>0</cache>
      <customDisplay>{{velocity}}
  $xwiki.jsrx.use('metabolites/widgets/sortConfiguration.js', {'minify' : false})
  $xwiki.ssx.use('PhenoTips.MetaboliteConfiguration')
  #set ($orderHelp = 'When a spreadsheet is uploaded, the system needs to know how the data is organized. Please arrange (drag and drop) the labels in the exact order that they appear in the spreadsheet.')
  #set ($typesHelp = 'These are all the types of columns within a spreadsheet that the system can recognize. For example, "Metabolite name" should match the column in the spreadsheet containing the names of metabolites tested.')
  #set ($available = {'metabolite_name': 'Metabolite name', 'specimen': 'Specimen', 'value': 'Value', 'unit': 'Unit', 'dob': 'Date of birth', 'sex': 'Sex', 'normal_low': 'Normal range low', 'normal_high': 'Normal range high', 'sd': "Standard Deviation"})
{{html wiki=true clean=false}}
&lt;div class='sort-dd-configuration-holder #if ($sortOnly) sort-only #end'&gt;
  &lt;input type='hidden' #if (!$!{sortOnly}) name="$prefix$name" #end value="#foreach ($option in $value) $option, #end"/&gt;
  &lt;div&gt;
    &lt;p&gt;&lt;span class='hint'&gt;Spreadsheet column order:&lt;/span&gt;
    &lt;span class="fa fa-question-circle xHelpButton controlled" title="$orderHelp"&gt;&lt;/span&gt;&lt;/p&gt; 
    &lt;ul class='sortable sort-configuration sort-configuration-connected'&gt;
      #foreach($option in $value)
        ## How is this going to work with ignored columns
        #set ($class = '')
        #set ($display = $available.remove($option))
        #if ("$!{option}" == '-') 
          #set ($display = 'Ignore')
          #set ($class = 'permanent-option')
        #end
        &lt;li class='$class'&gt;&lt;span class='sort-option-value'&gt;$option&lt;/span&gt;$display&lt;/li&gt;
      #end
    &lt;/ul&gt;
  &lt;/div&gt;
##
  &lt;div&gt;
    &lt;p&gt;&lt;span class='hint'&gt;Column types:&lt;/span&gt;
    &lt;span class="fa fa-question-circle xHelpButton controlled" title="$typesHelp"&gt;&lt;/span&gt;&lt;/p&gt;
    &lt;ul class='sortable sort-configuration-options sort-configuration-connected'&gt;
      #foreach($option in $available.entrySet())
        &lt;li&gt;&lt;span class='sort-option-value'&gt;$option.getKey()&lt;/span&gt;$option.getValue()&lt;/li&gt;
      #end
      &lt;li class='permanent-option'&gt;&lt;span class='sort-option-value'&gt;-&lt;/span&gt;Ignore&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/div&gt;
&lt;/div&gt;
{{/html}}{{/velocity}}</customDisplay>
