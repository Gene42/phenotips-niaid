    <property>
      <code>// stick into a function to not pollute global
var metabolitesUploadIframe = $('metabolites-upload-container').down('iframe');
var metaboliteTestsContainer = $$('.metabolite-tests')[0];
var columnOrder = metaboliteTestsContainer.down('.sort-dd-configuration-holder input');
var dateContainer = metaboliteTestsContainer.down('#metabolites-datepicker');
var date = dateContainer.down('input');
var dateError = dateContainer.next('.errormessage');
var patientId = metaboliteTestsContainer.down('input[name="patient_id"]').getValue();

// clear date. It is saved, but should not be
//date.setValue('');

date.observe('focus', function() {
  dateError.setStyle({'display': 'none'});
});

metabolitesUploadIframe.onload = function() {
  // get window and document objects for iframe (IE compatible)
  var metabolitesUploadIdoc = metabolitesUploadIframe.contentDocument || metabolitesUploadIframe.contentWindow.document;
  var fileSelect = metabolitesUploadIdoc.getElementById('metabolites-attachfile');

  var form = Element.up(fileSelect, 'form');
  var inputs = {'patientId': Element.down(form, 'input[name="patient_id"]'), 'date': Element.down(form, 'input[name="date"]'), 'columnOrder': Element.down(form, 'input[name="column_order"]'),
    'file': Element.down(form, 'input[name="file"]'), 'filepath': Element.down(form, 'input[name="filepath"]')};
  // removing overflow
  Element.setStyle(form, {'margin': '0'});

  Form.Element.setValue(inputs['patientId'], patientId);
  Element.observe(fileSelect, 'click', function(event) {
    if (!date.getValue()) {
      event.preventDefault();
      Event.element(event).disabled = true;
      Event.stop(event);
      dateError.setStyle({'display': 'block'});
      Event.element(event).disabled = false;
      return false;
    }
    return true;
  });
  Element.observe(fileSelect, 'change', function (event) {
    Form.Element.setValue(inputs['date'], date.getValue());
    Form.Element.setValue(inputs['columnOrder'], columnOrder.getValue());
    Form.Element.setValue(inputs['filepath'], Form.Element.getValue(inputs['file']));
    //if (Element.getValue(fileSelect)
    form.submit();
  });
};</code>
