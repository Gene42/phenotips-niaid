{{velocity}}&#xd;
#set ($format = "$!{request.f}")&#xd;
#if ($format != 'raster')&#xd;
  #set ($format = 'vector')&#xd;
#end&#xd;
#set ($standalone = ($format != '' &amp;&amp; "$!{request.standalone}" != '' &amp;&amp; "$!{request.standalone}" != '0' &amp;&amp; "$!{request.standalone}" != 'false'))&#xd;
