  <content>{{velocity output="false"}}
#set ($displayDocumentName = 'PhenoTips.PedigreeConfiguration')
#set ($displayDocument = $xwiki.getDocument($displayDocumentName))
#set ($mainConfigurationClassname = 'PhenoTips.PedigreeGlobalSettings')
#set ($existingConfigs = $preferencesDoc.getObjects($mainConfigurationClassname))

$xwiki.ssx.use($displayDocumentName)##
{{/velocity}}

{{velocity}}
{{html clean="false"}}
&lt;h2&gt;Pedigree configuration&lt;/h2&gt;
&lt;form action="${preferencesDoc.getURL('save')}" method="post"&gt;
&lt;div class="hidden"&gt;
    &lt;input type="hidden" name="form_token" value="${escapetool.xml($services.csrf.token)}" /&gt;
    &lt;input type="hidden" name="classname" value="${mainConfigurationClassname}" /&gt;
    &lt;input type="hidden" name="xredirect" value="${doc.getURL($xcontext.action, $request.queryString)}" /&gt;
  &lt;/div&gt;
  Use brackets-out symbol for adopted-out nodes: ##
{{/html}}##
    #set ($targetObj = $doc.getObject($mainConfigurationClassname))
    #if (!$targetObj)
        #set ($targetObj = $doc.newObject($mainConfigurationClassname))
        $doc.save('Added receive patient configuration', true)
    #end
    $doc.display("nonStandardAdoptedOutGraphic", "edit", $targetObj)
{{html clean="false"}}
  Propagate father's last name to descendants: ##
{{/html}}##
    $doc.display("propagateFatherLastName", "edit", $targetObj)
{{html clean="false"}}
  &lt;br&gt;&lt;br&gt;
  Date input format: ##
{{/html}}##
    $doc.display("dateInputFormat", "edit", $targetObj)
{{html clean="false"}}
  Date display format: ##
{{/html}}##
    $doc.display("dateDisplayFormat", "edit", $targetObj)
{{html clean="false"}}
&lt;br&gt;&lt;br&gt;
&lt;div class="buttons"&gt;
    &lt;input class="button" type="submit" value="Save" /&gt;
  &lt;/div&gt;
&lt;/form&gt;
{{/html}}
{{/velocity}}</content>
