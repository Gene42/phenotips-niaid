}
body .xwiki-livetable-topfilters-tip .tippointer div, body .xwiki-livetable-tagcloud-tip .tippointer div {
  border-top-color: $theme.backgroundSecondaryColor;
  top: -22px;
}
body .xwiki-livetable-topfilters-container .value-cloud-tools a {
  color: $theme.textColor;
  font-weight: bold;
}
#patients.xwiki-livetable .actions {
  text-align: right;
}
h1:target, h2:target, h3:target {
  background-color: $theme.highlightColor;
}
#globallinks {
  display: none;
}

