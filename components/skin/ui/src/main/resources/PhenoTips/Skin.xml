  margin-left: 0;
  margin-right: 5px;
  margin-top: 0.05em;
  min-height: 22px;
  padding: 0 3px;
  width: 20em;
}

#phenotips-globalTools label {
  margin-left: 2em;
}

#create-patient-record {
