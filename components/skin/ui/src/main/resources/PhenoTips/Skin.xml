  text-decoration: none !important;
}
#company h1 {
  border: 0 none;
  color: $theme.pageBackgroundColor;
  font-size: 1.2em;
  margin: 0 20px 0;
  position: absolute;
  right: 5%;
  text-shadow: 0 0 6px #777;
  text-transform: uppercase;
}
.xdocCreation {
  float: right;
  width: 48%;
}
#document-title h1 {
  margin: 0 0 0.5em;
  padding: 5px 0 0;
}
#mainContentArea, #mainEditArea {
  overflow: visible;
  padding: 10px;
  position: relative;
}

.actionmenu {
  height: 1.8em;
}
#phenotips-mainmenu, #phenotips-globalTools {
  background-color: rgba(255, 255, 255, 0.1);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0.2);
